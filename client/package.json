{
  "name": "portfolio-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node-esm src/utils/form-handler.tsx",
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "tsc && vite build && node scripts/inline-critical-css.mjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-critical-css": "critical index.html --base dist/ --inline --width 1300 --height 900 --target dist/index.html"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "aos": "^2.3.4",
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.3",
    "clean-css": "^5.3.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "gh-pages": "^6.0.0",
    "nodemailer": "^6.9.5",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.16.0",
    "react-router-dom": "^6.16.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@types/aos": "^3.0.7",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.3",
    "@types/nodemailer": "^6.4.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "rollup-plugin-critical": "^1.0.13",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
